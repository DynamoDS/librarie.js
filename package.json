{
  "name": "dynamo-hosted-contents",
  "description": "Project that contains all hosted contents of Dynamo Windows client",
  "main": "index.js",
  "scripts": {
    "test": "jest --coverage",
    "utiltest": "mocha -r ts-node/register __tests__/mochatest/libraryUtilitiesTest.ts --reporter mocha-junit-reporter --reporter-options mochaFile=./TestResults/MochaReport.xml",
    "build": "npm run dev & npm run prod",
    "dev": "cross-env NODE_ENV=development webpack",
    "prod": "cross-env NODE_ENV=production webpack"
  },
  "keywords": [
    "Dynamo",
    "Package Manager"
  ],
  "author": "Dynamo Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.14.0",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "underscore": "^1.12.1"
  },
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/enzyme": "^3.10.8",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "^21.0.0",
    "@types/node": "^7.0.5",
    "@types/react": "^15.0.0",
    "@types/react-dom": "^0.14.20",
    "@types/react-test-renderer": "^15.5.0",
    "@types/underscore": "^1.11.0",
    "awesome-typescript-loader": "^3.0.0-beta.18",
    "babel-jest": "^21.0.0",
    "chai": "^3.5.0",
    "core-js": "^3.6.0",
    "cross-env": "^3.2.4",
    "css-loader": "^1.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.5",
    "enzyme-to-json": "^3.6.1",
    "file-loader": "^0.10.1",
    "jest": "^21.0.0",
    "jest-jenkins-reporter": "^1.0.2",
    "mocha": "^8.0.0",
    "mocha-junit-reporter": "^1.23.1",
    "react-test-renderer": "^16.1.0",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.15.0",
    "ts-jest": "^21.0.0",
    "ts-node": "^8.3.0",
    "typescript": "3.5.3",
    "webpack": "^2.2.1"
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/*/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.ts",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.ts"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}"
    ],
    "testPathIgnorePatterns": [
      "__tests__/data/"
    ],
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "mapCoverage": true,
    "testResultsProcessor": "jest-jenkins-reporter"
  },
  "jestSonar": {
    "reportPath": "TestResults",
    "reportFile": "TestReport.xml",
    "indent": 4
  }
}
