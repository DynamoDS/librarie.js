{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "strict": false,
        "module": "commonjs",
        "target": "es2018",
        "jsx": "react",
        "lib": ["es2020", "dom"],
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "resolveJsonModule": true,
        "moduleResolution": "node"
    },
    "exclude": [
        "node_modules"
    ],
    "files": [
        "./src/LibraryUtilities.ts"
    ]
}